library(devtools)
library(roxygen2)

setwd("~/Dropbox/YT_Lab/MACtest/")
document()
setwd("..")
install("MACtest")
library(MACtest)
HA=sim_mediation_data(hypo="HA",mm=0.1,vv=0.1,sm=10)
PS=pre_stat(HA$S,HA$M,HA$Y,HA$X)
VCTg=GCN_VCT(HA$S,HA$M,HA$Y,HA$X,PS)
mnPg=GCN(PS,"minP")
CSp=cn_stat(PS,trans = T)
MPp=LCN(CSp,"minP")

library(devtools)
